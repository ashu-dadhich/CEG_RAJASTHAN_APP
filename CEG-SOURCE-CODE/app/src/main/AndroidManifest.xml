<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.ceg">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:background="#6CC417"
        android:icon="@mipmap/newceg"
        android:label="Center For Electronic Governance "
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splash"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Partners"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Regular_courses"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activities"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Credit"
            android:screenOrientation="portrait" />
        <activity
            android:name=".More"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Placement"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MyWebView"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Registration"
            android:screenOrientation="portrait" />
        <activity android:name=".about" />
        <activity android:name=".news"></activity>
    </application>

</manifest>