<?xml version="1.0" encoding="utf-8"?>

    <LinearLayout android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:android="http://schemas.android.com/apk/res/android">

        <ImageView
            android:id="@+id/logoId"
            android:clickable="true"
            android:layout_width="350dp"
            android:layout_height="100dp"
            android:src="@drawable/bautodesk"
            android:layout_weight="1"
            android:layout_gravity="left"
            android:layout_marginLeft="-50dp" />

        <ImageView
            android:id="@+id/logoId1"
            android:layout_width="350dp"
            android:clickable="true"
            android:layout_height="100dp"
            android:src="@drawable/boracle"
            android:layout_marginLeft="50dp"
            android:layout_weight="1"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:paddingRight="10dp"
            android:paddingBottom="5dp"
            android:layout_gravity="right" />

        <ImageView
            android:id="@+id/logoId2"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:src="@drawable/bcisco"
            android:layout_marginLeft="-50dp"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:paddingRight="10dp"
            android:paddingBottom="5dp"
            android:layout_gravity="left" />

        <ImageView
            android:id="@+id/logoId3"
            android:layout_width="383dp"
            android:layout_height="100dp"
            android:src="@drawable/amicrosoft2"
            android:paddingLeft="30dp"
            android:paddingBottom="0dp"
            android:paddingRight="30dp"
            android:layout_gravity="right"
            android:layout_marginLeft="50dp" />

        <ImageView
            android:id="@+id/logoId4"
            android:layout_width="350dp"
            android:layout_height="100dp"
            android:src="@drawable/bredhat2"
            android:layout_marginLeft="50dp"
            android:layout_weight="1"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:paddingRight="10dp"
            android:paddingBottom="5dp"
            android:layout_gravity="left" />

        <ImageView
            android:id="@+id/logoId5"
            android:layout_width="350dp"
            android:layout_height="100dp"
            android:src="@drawable/brkcl2"
            android:layout_weight="1"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:paddingRight="10dp"
            android:paddingBottom="5dp"
            android:layout_gravity="right"
            android:layout_marginRight="50dp" />



        <ImageView
            android:id="@+id/logoId6"
            android:layout_width="350dp"
            android:layout_height="100dp"
            android:src="@drawable/brsldc"
            android:paddingTop="5dp"
            android:paddingRight="10dp"
            android:paddingBottom="5dp"
            android:paddingLeft="25dp"
            android:layout_marginLeft="30dp" />
    </LinearLayout>

